.van-popup {
    border-radius: 10rpx;
}

.bindCon {
    width: 600rpx;
    border-radius: 10rpx;
    margin-bottom: 44rpx;
}

.bindCon .title {
    width: 100%;
    height: 88rpx;
    text-align: center;
    font-size: 28rpx;
    color: #000000;
    line-height: 84rpx;
    border-bottom: 2rpx solid #f2f1f6;
}

.bindCon .img {
    width: 100%;
    margin-top: 24rpx;
    text-align: center;
}

.bindCon .img image {
    width: 304rpx;
    height: 180rpx;
    display: inline-block;
}

.goouc_dati {
    width: 100%;
    display: flex;
    justify-content: space-around;
    text-align: center;
    margin-top: 50rpx;
}

.goouc_dati .goouc_datinum {
    flex: 0 1 auto;
}

.goouc_dati .goouc_datinum .goouc_num {
    font-size: 28rpx;
    color: #5c5c5c;
}

.goouc_dati .goouc_datinum .goouc_tit {
    margin-top: 16rpx;
    font-size: 28rpx;
    color: #5c5c5c;
}

.goouc_btn {
    width: 100%;
    padding-top: 56rpx;
    display: flex;
    justify-content: space-around;
}

.goouc_btn>button {
    width: 210rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    border-radius: 40rpx;
    border: 2rpx solid #4e9bf5;
}

.goouc_btn .goouc_btn_lt {
    background: #FFFFFF;
    color: #4e9bf5;
}

.goouc_btn .goouc_btn_rt {
    background: #4e9bf5;
    color: #fff;
}