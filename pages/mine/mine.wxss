.title-bar {
    padding-left: 50rpx;
    background: rgba(0,0,0,0);
}

.bindCon {
    width: 100%;
    padding-bottom: 12rpx;
    box-sizing: border-box;
}

.bindCon .title {
    width: 100%;
    height: 84rpx;
    text-align: center;
    font-size: 28rpx;
    color: #000000;
    line-height: 84rpx;
    border-bottom: 2rpx solid #f2f1f6;
    margin-bottom: 26rpx;
}

.bindCon .info {
    width: 100%;
    padding: 24rpx 36rpx 50rpx;
    box-sizing: border-box;
}

.bindCon .info .titinfo {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #7f808e;
}

.bindCon .info .titinfo video {
    width: 100%;
}

.mine_option_wrap {
    position: relative;
    z-index: 9;
}

.btnShare {
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: 0;
    z-index: 999;
    left: 0;
    top: 0;
}

.my_bl_block {
    position: absolute;
    height: 30px;
    width: 100%;
    overflow: hidden;
    bottom: 0;
    left: 0;
}

.my_bl_block .bl_block {
    position: absolute;
    width: 200%;
    height: 30px;
    background-size: 100% 100%;
    background-repeat: repeat;
    background-image: url(data:image/png;base64,);
    top: 0px;
    left: -100%;
    animation: water-right 10s infinite linear;
}

.my_bl_block .bl_block_one {
    top: 0px;
    background-image: url(data:image/png;base64,);
}

@keyframes water-right {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(25%);
    }

    100% {
        transform: translateX(50%);
    }
}