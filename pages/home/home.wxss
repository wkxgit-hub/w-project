.title-bar {
    color: #ffffff;
    padding-left: 25px;
}

.tab-one-bg {
    width: 100%;
    background: #fff;
    text-align: center;
    font-size: 16px;
    color: #666;
    box-sizing: border-box;
}

.tab-one-bg .tab-one-bg-swiper {
    width: 100%;
    height: 200px;
}

.tab-one-bg image {
    width: 100%;
}

.tab-one-bg-swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
}

.tab-one-bg-swiper .wx-swiper-dot {
    width: 14rpx;
    display: inline-flex;
    height: 14rpx;
    margin-left: 20rpx;
    justify-content: space-between;
}

.tab-one-bg-swiper .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    background: rgba(255,255,255,0.8);
    border-radius: 50%;
}

.tab-one-bg-swiper .wx-swiper-dot-active::before {
    background: rgb(255,201,82);
}

.margin_top {
    margin-top: 30rpx;
}

.img_icon {
    border-radius: 35rpx;
}

.day-rate {
    position: relative;
    overflow: hidden;
    padding: 8rpx 0;
}

.day-rate .center {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    width: 8rpx;
    height: 80rpx;
    background: #4e9bf5;
}

.day-rate .flex {
    float: left;
    width: 50%;
    text-align: center;
}

.day-rate .flex .top {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 14px;
    color: #666666;
}

.day-rate .flex .mid {
    height: 36rpx;
    line-height: 36rpx;
    font-size: 14px;
    color: #4e9bf5;
}

.day-rate .flex .bot {
    height: 40rpx;
    line-height: 36rpx;
    font-size: 12px;
    color: #898989;
}

.jhm-wrap {
    padding: 0 78rpx 0 72rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #f5f5f5;
}

.jhm-wrap .jhm {
    font-size: 28rpx;
    color: #666666;
    height: 60rpx;
    line-height: 60rpx;
    overflow: hidden;
}

.jhm-wrap .jhm .lt {
    float: left;
}

.jhm-wrap .jhm .rt {
    float: right;
    font-size: 24rpx;
    color: #999999;
}

.jhm-wrap .jhm .rt image {
    display: inline-block;
    width: 9rpx;
    height: 15rpx;
    margin-left: 23rpx;
}

button {
    background: #ffffff;
    padding: 0;
    margin: 0;
    font-size: 14px;
    line-height: 36rpx;
}